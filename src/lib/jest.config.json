{
  "setupFilesAfterEnv": ["<rootDir>/test-setup.ts"],
  "moduleNameMapper": {
    "\\.css$": "<rootDir>/__mocks__/styleMock.js"
  },
  "coverageThreshold": {
    "global": {
      "statements": 100,
      "branches": 100,
      "functions": 100,
      "lines": 100
    }
  },
  "coverageReporters": [
    "text",
    "lcov"
  ],
  "collectCoverageFrom": [
    "utils/**/*.ts",
    "notification-center/**/*.tsx",
    "notification-center/**/*.ts",
    "!utils/testUtils.ts"
  ],
  "coverageDirectory": "../../coverage/react"
}